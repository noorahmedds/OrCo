{
    "method": "bayes",
    "metric": {
      "name": "avg_hm",
      "goal": "maximize"
    },
    "parameters": {
        "student_temp":{
            "distribution": "uniform",
            "min": 0.09, "max": 0.15
        },
        "teacher_temp":{
            "distribution": "uniform",
            "min": 0.05, "max": 0.09
        },
        "warmup_teacher_temp":{
            "distribution": "uniform",
            "min": 0.04, "max": 0.07
        },
        "warmup_teacher_temp_epochs":{
            "distribution": "int_uniform",
            "min": 5, "max": 50
        },
        "min_lr":{
            "distribution": "uniform",
            "min": 0.0005, "max": 0.05
        },
        "warmup_epochs":{
            "distribution": "int_uniform",
            "min": 5, "max": 50
        },
        "weight_decay_end":{
            "distribution": "uniform",
            "min": 0.0001, "max": 0.01
        },
        "momentum_teacher":{
            "distribution": "uniform",
            "min": 0.9, "max": 0.999
        },
        "classifier_last_layer":{
            "values":["linear", "projection"]
        }
    }
}